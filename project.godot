; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Card",
"language": "GDScript",
"path": "res://card_system/card.gd"
}, {
"base": "Node",
"class": "Deck",
"language": "GDScript",
"path": "res://card_system/deck.gd"
}, {
"base": "Control",
"class": "Holder",
"language": "GDScript",
"path": "res://card_system/holder.gd"
}, {
"base": "Area2D",
"class": "HolderArea",
"language": "GDScript",
"path": "res://card_system/holder_area.gd"
}, {
"base": "AudioStreamPlayer",
"class": "SfxPlayer",
"language": "GDScript",
"path": "res://sfx_player.gd"
} ]
_global_script_class_icons={
"Card": "",
"Deck": "",
"Holder": "",
"HolderArea": "",
"SfxPlayer": ""
}

[application]

config/name="card_system"
run/main_scene="res://card_system/tests/tests.tscn"

[display]

window/size/width=900
window/size/resizable=false
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "gut" )

[rendering]

environment/default_clear_color=Color( 0.235294, 0.223529, 0.282353, 1 )
environment/default_environment="res://default_env.tres"
